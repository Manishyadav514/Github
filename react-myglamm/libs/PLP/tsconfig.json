{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2015",
    "module": "esnext",
    "allowJs": false,
    "jsx": "preserve",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext", "WebWorker"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "paths": {
      "@libComponents/*": ["../UX/src/Components/*"],
      "@libConstants/*": ["../UX/src/Constants/*"],
      "@libAnalytics/*": ["../UX/src/Analytics/*"],
      "@libPages/*": ["../UX/src/pages/*"],
      "@libHooks/*": ["../UX/src/hooks/*"],
      "@libAPI/*": ["../UX/src/lib/*"],
      "@libStore/*": ["../UX/src/store/*"],
      "@libStyles/*": ["../UX/src/styles/*"],
      "@libUtils/*": ["../UX/src/utils/*"],
      "@libServices/*": ["../UX/src/services/*"],
      "@libLayouts/*": ["../UX/src/Layouts/*"],
      "@typesLib/*": ["../@types/*"],
      "@checkoutLib/*": ["../checkout/src/*"],
      "@productLib/*": ["../product/src/*"],
      "@layout/*": ["../../apps/m-web/src/Layouts/*"],
      "@components/*": ["../../apps/m-web/src/Components/*"]
    }
  },
  "exclude": [],
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
